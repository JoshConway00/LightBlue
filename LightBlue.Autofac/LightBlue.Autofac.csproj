<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <ProjectGuid>{74B1F607-28DD-4248-AF6A-492B03AD321A}</ProjectGuid>
    <TargetFramework>net48</TargetFramework>
    <SolutionDir Condition="$(SolutionDir) == '' Or $(SolutionDir) == '*Undefined*'">..\</SolutionDir>
    <AssemblyTitle>LightBlue.Autofac</AssemblyTitle>
    <Company>Colin David Scott</Company>
    <Product>LightBlue.Autofac</Product>
    <Description>Provides Autofac support for the LightBlue Azure development platform</Description>
    <Copyright>Copyright 2014-2015 LightBlue contributors</Copyright>
    <Version>1.1.21.0</Version>
    <AssemblyVersion>1.1.21.0</AssemblyVersion>
    <FileVersion>1.1.21.0</FileVersion>
    <OutputPath>bin\$(Configuration)\</OutputPath>
    <TreatWarningsAsErrors>true</TreatWarningsAsErrors>
    <Authors>Colin David Scott</Authors>
    <PackageLicenseUrl>https://github.com/LightBlueProject/LightBlue/blob/master/LICENCE</PackageLicenseUrl>
    <PackageProjectUrl>https://github.com/LightBlueProject/LightBlue</PackageProjectUrl>
    <PackageTags>azure</PackageTags>
  </PropertyGroup>
  <PropertyGroup Condition=" '$(Configuration)|$(Platform)' == 'Debug|AnyCPU' ">
    <DebugType>full</DebugType>
  </PropertyGroup>
  <PropertyGroup Condition=" '$(Configuration)|$(Platform)' == 'Release|AnyCPU' ">
    <DebugType>pdbonly</DebugType>
  </PropertyGroup>
  <Import Project="$(SolutionDir)\.nuget\NuGet.targets" Condition="Exists('$(SolutionDir)\.nuget\NuGet.targets')" />
  <Target Name="EnsureNuGetPackageBuildImports" BeforeTargets="PrepareForBuild">
    <PropertyGroup>
      <ErrorText>This project references NuGet package(s) that are missing on this computer. Enable NuGet Package Restore to download them.  For more information, see http://go.microsoft.com/fwlink/?LinkID=322105. The missing file is {0}.</ErrorText>
    </PropertyGroup>
    <Error Condition="!Exists('$(SolutionDir)\.nuget\NuGet.targets')" Text="$([System.String]::Format('$(ErrorText)', '$(SolutionDir)\.nuget\NuGet.targets'))" />
  </Target>
  <ItemGroup>
    <PackageReference Include="Autofac" Version="3.5.2" />
  </ItemGroup>
  <ItemGroup>
    <ProjectReference Include="..\LightBlue\LightBlue.csproj" />
  </ItemGroup>
</Project>